        <clipPath id="clip0_879_2203">
        <rect width="24" height="24" fill="white"/>
        </clipPath>
        </defs>
      </svg>
    </div>
    {%- for block in section.blocks -%}
      {%- case block.type -%}
        {%- when 'logo' -%}
          <a href="/" class="header-logo">
            <img class="black" src="{{ block.settings.logo | img_url: "master" }}" width="" height="">
            <img class="white" src="{{ block.settings.logo_white | img_url: "master" }}" width="" height="">
          </a>
        {%- when 'menu' -%}
          <div class="header-menu" style="gap:{{ block.settings.gap }}px">
            {% for link in block.settings.menu.links %}
              <div class="header_menu-item" class="{% if link.levels > 0 %} on-subcategory {% endif %}">
                {% if link.levels > 0 %}
                  <span class="sublinks-button ">{{- link.title | escape -}}</span>
                  <div class="sublinks-container close-sublink-swrapper">
                      {% for link in link.links %}
                        <span class="sublinks">{{- link.title | escape -}}</span>
                      {% endfor %}
                  </div>
                {% else %}
                  <a  href="{{ link.url }}">{{- link.title | escape -}}</a>
                {% endif %}
              </div>
            {% endfor %}
          </div>
        {%- when 'icons' -%}
          <div class="header-icons" style="gap:{{ block.settings.gap }}px">
            <div class="icon-search icon-header-pad">
              {{- 'icon-search.svg' | inline_asset_content -}}
            </div>
            <div class="icon-account icon-header-pad">
              {{ 'icon-account.svg' | inline_asset_content }}
            </div>
            <div class="icon-cart icon-header-pad">
              {{ 'icon-cart.svg' | inline_asset_content }}
            </div>
          </div>
      {%- endcase -%}
    {%- endfor -%}
  </header>
</div>

{% comment %} minicart {% endcomment %}
<div class="minicart-swrapper minicart-swrapper-close" id="minicart">
  <div class="minicart-content">
    <div class="minicart-barra-frete minicart-footer-shipping-bar">
      <span class="minicart-footer-shipping-bar-text"></span>
      <div class="minicart-footer-shipping-bar-progress">
        <span class="shipping-bar" id="shipping-bar"></span>
      </div>
    </div>
    <div class="minicart-products minicart-list-header"></div>
    <div class="minicart-footer minicart-footer-order-content">
      <div class="minicart-footer-price-content">
        <span>Total:</span>
        <span class="finish-price-order"></span>
      </div>
      <a href="/cart" class="minicart-footer-finish-order"> Finalizar compra </a>
      <button class="minicart-footer-return-shop close-minicart ">Continuar comprando</button>
    </div>
  </div>
</div>

<div id="menu-mobile-swrapper" class="menu-mobile-swrapper menu-mobile-swrapper-close">
    <div class="header-menu-mobile" style="gap:{{ block.settings.gap }}px">
        {%- for block in section.blocks -%}
            {%- case block.type -%}
            {%- when 'menu' -%}
                {% for link in block.settings.menu.links %}
                  <div class="header_menu-item" class="{% if link.levels > 0 %} on-subcategory-mobile {% endif %}">
                    {% if link.levels > 0 %}
                      <span class="sublinks-button">
                        {{- link.title | escape -}}                             
                        {{- 'arrow-menu-mobile-one.svg' | inline_asset_content -}}
                      </span>
                      <div class="sublinks-container close-sublink-swrapper">
                          <span class="close-sublinks-mobile">
                            {{- 'arrow-menu-mobile-two.svg' | inline_asset_content -}}
                            {{- link.title | escape -}}
                          </span>
                          {% for sublink in link.links %}
                            <span class="sublinks">{{- sublink.title | escape -}}</span>
                          {% endfor %}
                      </div>
                    {% else %}
                      <a  href="{{ link.url }}">{{- link.title | escape -}}</a>
                    {% endif %}
                  </div>
                {% endfor %}
            {%- endcase -%}
        {%- endfor -%}
    </div>
    <div class="menu-mobile-footer">
