{% style %}
  .header-section-container-{{ section.id }}{
      max-width:{{ section.settings.max-width }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
      padding-top: {{ section.settings.padding_top }}px;
      padding-right:{{ section.settings.padding_right }}px;
      padding-left:{{ section.settings.padding_left }}px;
      background:{{ section.settings.background }};
      border-bottom: 1px solid #D4D4D4;

  }
  .header-pad{
      display: flex;
      width: 100%;
      align-items: center;
      justify-content: space-between;
      max-width:{{ section.settings.max-width }}px;
      height:{{ section.settings.height }}px;
      margin:0 auto;
  }
  .header-menu{
      display: flex;
  }
  .header-icons{
      display: flex;
  }
  .header_menu-item a{
      font-family: Helvetica;
      font-size: 12px;
      font-weight: 400;
      line-height: 13.8px;
      text-align: left;
      text-transform: uppercase;
      text-decoration:none;
      color:#000;
  }
  .header_menu-item span{
      font-family: Helvetica;
      font-size: 12px;
      font-weight: 400;
      line-height: 13.8px;
      text-align: left;
      text-transform: uppercase;
      text-decoration:none;
      color:#000;
      cursor:pointer;
  }
  .header-logo{
      display:flex;
  }
  .minicart-swrapper{
      display: flex;
      position: fixed;
      width: 100%;
      height: 100%;
      top: 0;
      right:0;
      background: #1E1E1EA3;
      z-index: 99;
      justify-content: flex-end;
      transition: 0.7s;
  }
  .minicart-content{
      display: flex;
      width: 100%;
      background: #fff;
      max-width: 400px;
      flex-direction: column;

  }
  .minicart-product-card-container{
      display: flex;
      align-items: center;
      max-height: 105px;
      gap: 16px;
      border-bottom: 1px solid #E4E4E4;
      padding-bottom: 24px;
  }
  .minicart-product-card-texts-container{
      display: flex;
      align-items: center;
      max-height: 105px;
  }
  .minicart-product-card-img-content{
      width: 100%;
      max-width: 81px;
      max-height: 81px;
      overflow: hidden;
  }
  .minicart-product-card-texts-content{
      display: flex;
      align-items: flex-end;
      width: 100%;
      justify-content: space-between;
  }
  .minicart-product-card-texts-content p{
      font-family: Helvetica Neue;
      font-size: 16px;
      font-weight: 400;
      line-height: 20.8px;
      text-align: left;
      color: #000;
      margin: 0;
      width: 100px;
      overflow: hidden;
      height: 21px;
  }
  .minicart-product-card-texts-container .price-product{
      display: flex;
      height: fit-content;
      font-size: 16px;
      font-weight: 600;
      line-height: 24px;
      text-align: left;
  }
  .input-selector-quantity-minicart button{
      border: 1px solid #E4E4E4;
      height: 24px;
      width: 24px;
      background: transparent;
  }
  .input-selector-quantity-minicart input{
      background: transparent;
      border: none;
      height: 24px;
      width: 24px;
      padding: 0;
      text-align: center;
      justify-content: center;
  }
  .minicart-products{
      padding:16px;
  }
  .minicart-footer.minicart-footer-order-content{
      height: 174px;
      display: flex;
      flex-direction: column;
      align-items: center;
      border-top: 1px solid #E4E4E4;
      padding: 20px 28px;
      gap: 16px;

  }
  .minicart-footer-price-content{
      width: 100%;
      justify-content: space-between;
      display: flex;
  }
  .minicart-products.minicart-list-header{
      height: 100%;
  }
  .minicart-footer-return-shop{
      font-size: 14px;
      font-weight: 600;
      line-height: 21px;
      text-align: left;
      color: #222B3A;
      border: none;
      background: transparent;
      text-decoration: underline;

  }
  .minicart-footer-finish-order{
      width: 352px;
      height: 56px;
      padding: 20px 28px 20px 28px;
      gap: 10px;
      border-radius: 4px;
      border: none;
      background: #141F14;
      font-size: 16px;
      font-weight: 600;
      line-height: 19.36px;
      text-align: center;
      text-transform: uppercase;
      color: #FFFFFF;
      text-decoration: none;
  }
  .minicart-barra-frete.minicart-footer-shipping-bar{
      height: 102px;
      background: #141F14;
      display: flex;
      align-items: center;
      justify-content: center;
      flex-direction: column;
      gap:12px;
  }
  .shipping-bar{
      display: flex;
      width: 50%;
      height: 8px;
      border-radius: 32px;
      background: #fff;
  }
  .minicart-footer-shipping-bar-progress{
      width: 294px;
      height: 8px;
      background: #FFFFFF29;
      border-radius: 32px;
  }
  .minicart-footer-shipping-bar-text{
      font-family: Helvetica Neue;
      font-size: 14px;
      font-weight: 300;
      line-height: 18.2px;
      text-align: left;
      color: #fff;
  }
  .minicart-swrapper-close{
      right: -5000px;
      transition: 0.7s;
  }
  .mobile-menu{
      display:none;
  }
  .white{
    display:none
  }
  .menu-mobile-swrapper{
    display:none;
  }
  .sublinks-container{
    position: absolute;
    width: 100%;
    left: 0;
    background: #fff;
    padding: 32px 64px;
    margin-top: 15.5px;
    transition:1s;

  }
  .close-sublink-swrapper{
    top:-2000px;
    transition:1s;
  }
  .color-transparant{
    color:transparent !important;
  }
  @media screen and ( max-width: 750px ){
    .header-section-container-{{ section.id }}{
        padding-bottom: {{ section.settings.padding_bottom_mobile }}px;
    padding-top: {{ section.settings.padding_top_mobile }}px;
    padding-right:{{ section.settings.padding_right_mobile }}px;
    padding-left:{{ section.settings.padding_left_mobile }}px;
  }
    .mobile-menu{
     display:flex;
    }
    .header-menu{
     display:none;
    }
    .menu-mobile-swrapper-close {
        left:-2000px;
        transition:0.7s;
    }
    .menu-mobile-swrapper{
        display:flex;
        position: fixed;
        height: 85%;
        width: 100%;
        top: 0;
        background: #fff;
        flex-direction: column;    
        transition:0.7s;
        top: 15%;
        box-shadow: 4px 0px 0px 8px #43434A14;
    }
    .sublinks-container{
      display: flex;
        position: absolute;
        height: 85%;
        width: 100%;
        top: 0;
        background: #fff;
        flex-direction: column;
        transition: 0.7s;
        padding: 20px 32px;
        gap:20px;
    }
    .header-menu-mobile{
        width: 100%;
        height: 100%;
        padding: 20px 32px;
        gap: 36px;
        display: flex;
        flex-direction: column;
    }
    .header-menu-mobile .header_menu-item a{
        font-family: sans-serif;
        font-size: 16px;
        font-weight: 700;
        line-height: 20.02px;
        letter-spacing: 0.04em;
        text-align: left;
        text-decoration: none;
        color: #000001;
        text-transform: uppercase;
    }
    .menu-mobile-footer{
        width: 100%;
        height: 143px;
        background: #F8F8F8;
        padding: 24px 32px 40px 32px;
    }
    .menu-mobile-footer a{
        font-size: 14px;
    font-weight: 500;
    line-height: 16.94px;
    display: flex;
    text-align: left;
    align-items: center;
    gap: 12px;
    text-transform: capitalize;
    color: #231F20;
    text-decoration: none;
    }
    .footer-menu-mobile-midias{
        display: flex;
    width: 176px;
    justify-content: space-between;
    margin-top: 16px;
    margin-left: -6px;
    }
    .close-sublink-swrapper{
      left:-1000px;
    }
    .header-section-container.fixed-mobile{
      position: fixed;
    width: 100%;
    background: #fff;
    top: 36.8px;
    }
    .utility-bar.fixed-mobile{
      position: fixed;
      top: 0;
      height: fit-content;
      width: 100%;
      align-items: center;
      display: flex;
      padding: 0;
    }
    .header-menu-mobile .header_menu-item .sublinks-button{
      font-family: sans-serif;
        font-size: 16px;
        font-weight: 700;
        line-height: 20.02px;
        letter-spacing: 0.04em;
        text-align: left;
        text-decoration: none;
        color: #000001;
        text-transform: uppercase;
        display: flex;
        align-items: center;
        width: 100%;
        justify-content: space-between;
    }
  }

  {%- for block in section.blocks -%}
      {%- case block.type -%}
          {%- when 'icons' -%}
              .icon-header-pad{
                  height:{{ block.settings.height }}px;
              }
      {%- endcase -%}
  {%- endfor -%}
{% endstyle %}

{% comment %} header desktop {% endcomment %}
<div  class="header-section-container header-section-container-{{ section.id }}" quantity_frete_free="{{ section.settings.quantity_frete_free }}">
  <header class="header-pad header-desktop-pad">
    <div class="mobile-menu">
      <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
