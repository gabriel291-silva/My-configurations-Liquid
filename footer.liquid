{{ 'section-footer.css' | asset_url | stylesheet_tag }}

<footer
  class="footer-container"
  style="--background-footer-color: {{ section.settings.color_scheme.settings.text_second}}"
>
  <div class="footer-content">
    <div class="footer-categories-content">
      {% for link in section.settings.menu.links %}
        <div             
            id="{{ link.handle }}"
            class="footer-category-content">
          <div
            class="footer-category-title"
            style="--color-font: {{ section.settings.color_scheme.settings.text_reverse }}"
          >
            <span> {{ link.title }}</span> 
            <div class="category-footer-icon-accordion"> + </div>
            </div>
          <ul class="footer-category-list-container">
            {% for sublink in link.links %}
              <li class="footer-subcategory-content">
                <a
                  class="footer-subcategory-link"
                  href="{{ sublink.url }}"
                  style="--color-font: {{ section.settings.color_scheme.settings.text_reverse }}"
                >
                  {{ sublink.title }}
                </a>
              </li>
            {% endfor %}
          </ul>
        </div>
      {% endfor %}
    </div>
    <div class="footer-icons-content">
      <div class="footer-icons-contentainer-redes">
        <span 
            class="footer-icons-redes-title" 
            style="--color-font: {{ section.settings.color_scheme.settings.text_reverse }}" >
            {{ section.settings.title_redes }}
        </span>
        <div class="footer-icons-contentents-redes"> 
            <a href="">
                <img 
                 src="{{ section.settings.icon_fecebook | image_url}}" width="100%" height="100%" 
                 class="footer-icon-rede-img"
                 style="
                 --icon-size-desktop:{{ section.settings.icones_height_range }}px;
                 --icon-size-mobile:{{ section.settings.icones_height_range_mobile }}px;">
            </a>
            <a href="">
                <img 
                 src="{{ section.settings.icon_instagram | image_url}}" width="100%" height="100%" 
                 class="footer-icon-rede-img"
                 style="
                 --icon-size-desktop:{{ section.settings.icones_height_range }}px;
                 --icon-size-mobile:{{ section.settings.icones_height_range_mobile }}px;">
            </a>
            <a href="">
                <img 
                 src="{{ section.settings.icon_youtube | image_url}}" width="100%" height="100%" 
                 class="footer-icon-rede-img"
                 style="
                 --icon-size-desktop:{{ section.settings.icones_height_range }}px;
                 --icon-size-mobile:{{ section.settings.icones_height_range_mobile }}px;">
            </a>

        </div>
      </div>
      <div class="footer-icons-content-payments footer-desktop">
        <div class="footer-icons-contentainer-redes">
            <span 
                class="footer-icons-payment-title" 
                style="--color-font: {{ section.settings.color_scheme.settings.text_reverse }}" >
                {{ section.settings.title_payment }}
            </span>
            <div class="footer-icons-contentents-payment"> 
            {% for block in section.blocks %}
                <a href="{{ block.settings.icon_url }}" alt="{{ block.settings.icon_alt }}">
                    <img 
                     src="{{ block.settings.icon_img | image_url}}" width="100%" height="100%" 
                     class="footer-icon-payment-img"
                     style="
                     --icon-size-desktop:{{ section.settings.icones_height_range }}px;
                     --icon-size-mobile:{{ section.settings.icones_height_range_mobile }}px;">
                </a>
            {% endfor %}
            </div>
          </div>
        

      </div>
    </div>
  </div>
  <div class="links-sign-sell-content">
    <a
      href="{{ section.settings.link_sing }}"
      class="sign-button"
      style="
        --background-color: {{ section.settings.color_scheme.settings.text_second}};
        --border-color: {{ section.settings.color_scheme.settings.text_reverse}};
        --font-color: {{ section.settings.color_scheme.settings.text_reverse}};
      "
    >{{ section.settings.button_sing }}</a>
    <a href="{{ section.settings.link_sell }}"
      class="want-sell-button"
      style="
        --background-color: {{ section.settings.color_scheme.settings.text_reverse}};
        --font-color: {{ section.settings.color_scheme.settings.text_second}};
      "
    >{{ section.settings.button_sell }}</a>
  </div>
</footer>
<div class="bottom-footer-target-container">
    <div class="bottom-footer-target">
        <div class="bottom-footer-target-content">
            <div class="footer-icons-content-payments footer-mobile">
                <div class="footer-icons-contentainer-redes">
                    <span 
                        class="footer-icons-payment-title" 
                        style="--color-font: {{ section.settings.color_scheme.settings.text_reverse }}" >
                        {{ section.settings.title_payment }}
                    </span>
                    <div class="footer-icons-contentents-payment"> 
                    {% for block in section.blocks %}
                        <a href="{{ block.settings.icon_url }}" alt="{{ block.settings.icon_alt }}">
                            <img 
                             src="{{ block.settings.icon_img_mobile | image_url}}" width="100%" height="100%" 
                             class="footer-icon-payment-img"
                             style="
                             --icon-size-desktop:{{ section.settings.icones_height_range }}px;
                             --icon-size-mobile:{{ section.settings.icones_height_range_mobile }}px;">
                        </a>
                    {% endfor %}
                    </div>
                  </div>
                
        
            </div>
            <div class="button-return-top footer-mobile">
